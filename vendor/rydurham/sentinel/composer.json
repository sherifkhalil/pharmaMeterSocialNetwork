{
    "name": "rydurham/sentinel",
    "description": "An implementation of the Sentry User Manager for Laravel.",
    "keywords": ["laravel", "sentry", "auth"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ryan Durham",
            "email": "ryan@stagerightlabs.com",
            "homepage": "http://stagerightlabs.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.2.*@dev",
        "illuminate/contracts": "5.2.*@dev",
        "cartalyst/sentry": "2.1.*",
        "vinkla/hashids": "~2.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.2.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "classmap": [
            "src/seeds",
            "src/controllers",
            "tests"
        ],
        "psr-4": {
            "Sentinel\\": "src/Sentinel"
        }
    },
    "minimum-stability": "stable"
}
